<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
	<servers>
		<server>
			<id>oss-jfrog-snapshots</id>
			<username>${env.OSS_JFROG_API_USER}</username>
			<password>${env.OSS_JFROG_API_KEY}</password>
		</server>
		<server>
			<id>oss-jfrog-central</id>
			<username>${env.OSS_JFROG_API_USER}</username>
			<password>${env.OSS_JFROG_API_KEY}</password>
		</server>
	</servers>
	<profiles>
		<profile>
			<repositories>
				<repository>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>bintray-thrxorg-public-maven-repo</id>
					<name>bintray</name>
					<url>http://dl.bintray.com/thrxorg/public-maven-repo</url>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>bintray-thrxorg-public-maven-repo</id>
					<name>bintray-plugins</name>
					<url>http://dl.bintray.com/thrxorg/public-maven-repo</url>
				</pluginRepository>
			</pluginRepositories>
			<id>bintray-thrxorg</id>
		</profile>
		<profile>
			<repositories>
				<repository>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>oss-jfrog-central</id>
					<name>libs-release</name>
					<url>https://oss.jfrog.org/artifactory/libs-release</url>
				</repository>
				<repository>
					<snapshots />
					<id>oss-jfrog-snapshots</id>
					<name>libs-snapshot</name>
					<url>https://oss.jfrog.org/artifactory/libs-snapshot</url>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>oss-jfrog-central</id>
					<name>plugins-release</name>
					<url>https://oss.jfrog.org/artifactory/plugins-release</url>
				</pluginRepository>
				<pluginRepository>
					<snapshots />
					<id>oss-jfrog-snapshots</id>
					<name>plugins-snapshot</name>
					<url>https://oss.jfrog.org/artifactory/plugins-snapshot</url>
				</pluginRepository>
			</pluginRepositories>
			<id>oss-jfrog-org</id>
		</profile>
	</profiles>

	<activeProfiles>
		<activeProfile>bintray-thrxorg</activeProfile>
		<activeProfile>oss-jfrog-org</activeProfile>
	</activeProfiles>

</settings>

